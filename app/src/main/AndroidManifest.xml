<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.usercollegeapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/soceticon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/soceticon"
        android:supportsRtl="true"
        android:theme="@style/Theme.UserCollegeApp">
        <activity android:name=".authentication.ForgetPassActivity"></activity>
        <activity android:name=".authentication.LoginActivity" />
        <activity android:name=".authentication.RegisterActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DeveloperActivity" />
        <activity android:name=".FullImageView" />
        <activity android:name=".ebook.PdfViewerActivity" />
        <activity
            android:name=".ebook.EbookActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>